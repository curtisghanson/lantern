{# src/Lantern/AuthBundle/Resources/views/Account/register.html.twig #}
{% extends '::base.html.twig' %}
{% block content %}
{{ form_start(form) }}
    {{ form_errors(form) }}

    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    <button type="submit" class="btn">register</button>

{{ form_end(form) }}
{% endblock content %}